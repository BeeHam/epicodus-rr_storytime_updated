<h1>Storytime!</h1>
<div class="row">
  <% @stories.each do |story| %>
    <div class="col-md-4">
      <div class="content">
        <img src="<%= story.image %>">
        <p><%= link_to story.title, story_path(story) %> | <%= link_to "edit", edit_story_path(story) %></p>
      </div>
    </div>
  <% end %>
</div>

<%= link_to "Add a Story", new_story_path %>
